{
  "name": "@unvuetify/nuxt-utils",
  "type": "module",
  "version": "0.0.1",
  "description": "Nuxt 3 utilities for Vuetify",
  "author": "userquin <userquin@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/userquin/unvuetify#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/userquin/unvuetify.git",
    "directory": "packages/nuxt-utils"
  },
  "bugs": {
    "url": "https://github.com/userquin/unvuetify/issues"
  },
  "keywords": [
    "nuxt",
    "nuxt 3",
    "vuetify",
    "vuetify 3",
    "auto-import",
    "sass",
    "scss"
  ],
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.mjs",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub",
    "test:typecheck": "tsc --noEmit",
    "test:attw": "attw --pack --config-path ../../.attw-esm-only.json"
  },
  "dependencies": {
    "@unvuetify/shared": "workspace:*",
    "@unvuetify/unimport-presets": "workspace:*",
    "@unvuetify/vite-styles-plugin": "workspace:*"
  },
  "devDependencies": {
    "@nuxt/schema": "catalog:"
  }
}
