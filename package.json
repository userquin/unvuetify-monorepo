{
  "name": "@unvuetify/monorepo",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@10.8.1",
  "description": "Unified Vuetify utilities",
  "author": "userquin <userquin@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/userquin/unvuetify#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/userquin/unvuetify.git"
  },
  "bugs": {
    "url": "https://github.com/userquin/unvuetify/issues"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "nr lint --fix",
    "build": "pnpm -r --filter=\"./packages/**/\" run build",
    "stub": "pnpm -r --filter=\"./packages/**/\" run stub",
    "typecheck": "tsc --noEmit",
    "release": "bumpp -r --excute=\"pnpm i\"",
    "local:tgz": "pnpm -r --parallel --filter=\"./packages/**/\" run local:tgz",
    "test:attw": "pnpm -r --parallel --filter=\"./packages/**/\" test:attw",
    "prepare-stackblitz-script": "tsx ./scripts/prepare-stackblitz-script.ts"
  },
  "devDependencies": {
    "@antfu/eslint-config": "catalog:",
    "@antfu/ni": "catalog:",
    "@arethetypeswrong/cli": "catalog:",
    "@nuxt/module-builder": "catalog:",
    "@types/node": "catalog:",
    "bumpp": "catalog:",
    "eslint": "catalog:",
    "pathe": "catalog:",
    "read-yaml-file": "catalog:",
    "taze": "catalog:",
    "tsx": "catalog:",
    "typescript": "catalog:",
    "unbuild": "catalog:",
    "unimport": "catalog:",
    "unplugin-auto-import": "catalog:",
    "unplugin-vue-components": "catalog:",
    "upath": "catalog:",
    "vue-tsc": "catalog:",
    "vuetify": "catalog:"
  },
  "resolutions": {
    "nuxt": "catalog:",
    "vite": "catalog:",
    "vuetify": "catalog:"
  },
  "stackblitz": {
    "installDependencies": false,
    "startCommand": "node .stackblitz.js && pnpm install && nr stub"
  }
}
