{
  "name": "basic-nuxt",
  "type": "module",
  "version": "1.1.3",
  "private": true,
  "description": "Nuxt playground auto-importing Vuetify components, directives and composables.",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "prepare": "nuxt prepare",
    "test:typecheck": "TYPECHECK=true nuxt prepare && TYPECHECK=true nuxt typecheck"
  },
  "dependencies": {
    "@mdi/font": "catalog:",
    "nuxt": "catalog:",
    "vue": "catalog:",
    "vuetify": "catalog:"
  },
  "devDependencies": {
    "@nuxt/fonts": "catalog:",
    "@unvuetify/nuxt-utils": "workspace:*",
    "sass-embedded": "catalog:",
    "typescript": "catalog:",
    "unimport": "catalog:",
    "vite": "catalog:",
    "vue-tsc": "catalog:"
  },
  "stackblitz": {
    "installDependencies": false,
    "startCommand": "node .stackblitz.js && pnpm install && pnpm dev"
  }
}
