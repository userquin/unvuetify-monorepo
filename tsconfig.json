{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["ESNext"],
    "moduleDetection": "force",
    "module": "preserve",
    "moduleResolution": "Bundler",
    "paths": {
      "@unvuetify/nuxt-i18n-utils": [
        "./packages/nuxt-i18n-utils/src/"
      ],
      "@unvuetify/nuxt-utils": [
        "./packages/nuxt-utils/src/"
      ],
      "@unvuetify/shared": [
        "./packages/shared/src/"
      ],
      "@unvuetify/unimport-presets": [
        "./packages/unimport-presets/src/"
      ],
      "@unvuetify/unplugin-vue-components-resolvers": [
        "./packages/unplugin-vue-components-resolvers/src/"
      ],
      "@unvuetify/vite-styles-plugin": [
        "./packages/styles/src/"
      ]
    },
    "types": [
      "node"
    ],
    "strict": true,
    "declaration": true,
    "sourceMap": true,
    "esModuleInterop": false,
    "isolatedModules": true,
    "skipLibCheck": true
  }
}
